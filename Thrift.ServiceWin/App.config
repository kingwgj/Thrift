<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <appSettings>

    <add key="ThriftServerConfigPath" value="Config/ThriftServer.config"/>
    
    <!--zookeeper服务器-->
    <add key="ZookeeperHost" value="127.0.0.1:2181"/>
    <add key="ZookeeperSessionTimeout" value="5000"/>
    <add key="ZookeeperDigest" value="TestUser:123456"/>

    <!--thrift服务Ip限制配置文件路径-->
    <add key="IpLimitPath" value="Config/IpLimit.config"/>

  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
